<div class="wrapper">
<section class="clearfix">
  <article class="one_fourth">
  </article>
  <article class="one_half">

<form method="post" action="/register">
  <ul>
    <li>
      <label class="names" for="name">Username: </label>
      <input class = "names" type="text" name="name" id="name"/>
    </li>
    <li>
      <label class="names" for="password">Password: </label>
      <input class = "names" type="password" name="password" id="password"/>
    </li>
   </ul>
      <button class="loginbutton" type="submit">Submit</button>

      <div class = "red"><% if flash[:notice] %>
        <p><%= flash[:notice] %></p>
      <% end %>
      </div>
</form>

    </article>
    <article class="one_fourth">
    </article>
  </section>
</div>